{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
    "text": "GDP Growth",
    "anchor": "start",
    "subtitle": "Annually, % | Source: FRED through ECO API",
    "fontSize": 14,
    "subtitleFontSize": 12,
    "color": "#000000",
    "subtitleColor": "#000000"
  },
  "height": 400,  
  "width": 400,
  "autosize": {
    "type": "fit",
    "contains": "padding"
  },
  "background": "#ffffff",
  "view": {
    "fill": "#ffffff",
    "strokeOpacity": 0
  },
  "data": {
    "url": "https://api.economicsobservatory.com/usa/grow?vega",
    "format": {
      "type": "json"
    }
  },
  "mark": {
    "type": "line",
    "color": "#9a244f",
    "strokeWidth": 2  
  },
  "encoding": {
    "x": {
      "field": "date",
      "type": "temporal",
      "axis": {
        "title": "Year",
        "titleColor": "#122B39",
        "grid": false
      }
    },
    "y": {
      "field": "value",
      "type": "quantitative",
      "axis": {
        "title": "",
        "titleColor": "#122B39",
        "grid": false
      }
    }
  }
}
